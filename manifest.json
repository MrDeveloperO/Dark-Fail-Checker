{

    "manifest_version": 2,
    "name": "Dark Fail Checker",
    "version": "1.0",
    "description": "Check if onion address matches the address from dark fail",
  
    "browser_action": {
        "default_icon": "icon.jpg",
        "default_title": "Dark Fail",
        "default_popup": "index.html",
        "browser_style": true
      },

      "permissions": [
        "<all_urls>",
        "tabs",
        "webRequest",
        "storage"
      ]
  
  }